<expres><![CDATA[#$pyFunction
def GetLSProData(page_data,Cookie_Jar,m):
 import requests
 import re
 import base64

 headers = {'User-Agent': 'Mozilla/5.0 (X11; Linux i686; rv:42.0) Gecko/20100101 Firefox/42.0 Iceweasel/42.0', 'Referer' : 'http://tv-msn.com/ae.html'}
 url = 'http://tv-msn.com/ae.php'

 r1 = requests.get(url, headers=headers)

 source = r1.text
 streamfn = re.findall('streamer: (.*?)\(', source)[0]
 formula = re.search(r'function ' + streamfn + '.*\n.*\[(.*?)\].*?,.*?"(.*?)".*?\((.*?).join.*?ById.*?ById."(.*?)".*?ById.*?ById."(.*?)"', source)
 pos = int(re.findall('id=' + formula.group(2) + '.(.*?)..span', source)[0])
 par1 = formula.group(1).replace('\"', '').replace(',', '')
 pat1 = (par1[pos:] + par1[:pos]).replace('\\/', '/')
 par2 = (re.findall('var ' + formula.group(3) + '.*?(".*?)\]', source)[0]).replace('\"', '').replace(',', '')
 pat2 = par2[pos:] + par2[:pos]
 par3 = re.findall('id=' + formula.group(4) + '.(.*?)..span', source)[0]
 pat3 = par3[pos:] + par3[:pos]
 par4 = re.findall('id=' + formula.group(5) + '.(.*?)..span', source)[0]
 pat4 = par4[pos:] + par4[:pos]
 filefn = re.findall('file: (.*?)\(', source)[0]
 pat5 = (re.findall('function ' + filefn + '.*\n.*\[(.*?)\]', source)[0]).replace('\"', '').replace(',', '')
 pat6 = base64.b64decode('ZXNwZXJv')
 rtmppath = pat1 + pat2 + pat3 + pat4 + ' playpath=' + pat5 + ' token=' + pat6
 return rtmppath
